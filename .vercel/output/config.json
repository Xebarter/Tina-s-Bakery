{
  "version": 3,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api"
    },
    {
      "src": "/pesapal/(.*)",
      "dest": "/api/pesapal/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "s-maxage=0, stale-while-revalidate=60"
      }
    }
  ]
}