{
  "name": "tinas-bakery",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Tina's Bakery - A modern bakery e-commerce platform",
  "author": "Tina's Bakery Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "dev:server": "node server/index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "vite build",
    "build:server": "esbuild server/index.js --bundle --platform=node --target=node18 --outfile=.vercel/output/functions/server/index.js --packages=external",
    "start": "node server/index.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "vercel-build": "node vercel-build.mjs",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.43.0",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.20.2",
    "express": "^5.1.0",
    "lucide-react": "^0.414.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.22.0",
    "uuid": "^10.0.0",
    "vite": "^5.1.0",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.3.2"
  }
